<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="TransparentEditText" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/text_view_rectangle</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="WhiteButtonLittleTransparent" parent="@android:style/Widget.Button">
        <item name="android:background">@drawable/white_round_button_background</item>
        <item name="android:textColor">#170556</item>
        <item name="android:textSize">27sp</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>
